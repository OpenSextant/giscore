<?xml version="1.0" encoding="utf-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Placemark>
    <name>styled placemark</name>
    <Style>
      <IconStyle>
	<!--
		 no icon or label is displayed in google earth
		 google parses the color as blue and set opacity at 0 (100% transparent)
	 -->
        <color>ff0000</color> <!-- color omits alpha value -->
        <scale>1.1</scale>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/shapes/airports.png</href>
        </Icon>
      </IconStyle>
      <LabelStyle>
	<!-- no label displayed -->
        <color>ff0000</color> <!-- color omits alpha value -->
      </LabelStyle>
    </Style>
    <Point>
	<coordinates>48.11078528860224,29.00797279966015</coordinates>
    </Point>
  </Placemark>
</kml>
