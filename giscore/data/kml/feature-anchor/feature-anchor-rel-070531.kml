<?xml version="1.0" encoding="utf-8"?>
<!--

Feature anchor as in KML 2.2 May 31, 2007 spec

<a href="#my-office">my office</a>
<a href="eat-at-google.kml#plymouth-rocks">Plymouth rocks</a>
<a href="#my-office" rel="FlyTo">my office</a>
<a href="#my-office" rel="Balloon">my office</a>
<a href="#my-office" rel="BalloonFlyTo">my office</a>

-->
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Feature anchor w/ rel</name>
    <description>Feature anchor with action in rel attr</description>
    <Placemark id="my-office">
      <name>My office</name>
      <description>My office is in this building</description>
      <Point>
        <coordinates>-122.0821604290386,37.42041505714916</coordinates>
      </Point>
    </Placemark>
    <Placemark>
      <name>No rel</name>
      <description>
        <![CDATA[ 
        Fly to a feature in this file such as
        <a href="#my-office">my office</a>
        ]]>
      </description>
      <Point>
        <coordinates>-123.0821604290386,37.42041505714916</coordinates>
      </Point>
    </Placemark>
    <Placemark>
      <name>rel=FlyTo</name>
      <description>
        <![CDATA[ 
        Fly to a feature in this file such as
        <a href="#my-office" rel="FlyTo">my office</a>
        ]]>
      </description>
      <Point>
        <coordinates>-123.0821604290386,37.42041505714916</coordinates>
      </Point>
    </Placemark>
    <Placemark>
      <name>rel=balloon</name>
      <description>
        <![CDATA[ 
        Pop the balloon of a feature in this file such as
        <a href="#my-office" rel="Balloon">my office</a>
        ]]>
      </description>
      <Point>
        <coordinates>-123.0821604290386,37.42041505714916</coordinates>
      </Point>
    </Placemark>
    <Placemark>
      <name>rel=BalloonFlyTo</name>
      <description>
        <![CDATA[ 
        Fly to and pop the balloon of a feature in this file such as
        <a href="#my-office" rel="BalloonFlyTo">my office</a>
        ]]>
      </description>
      <Point>
        <coordinates>-123.0821604290386,37.42041505714916</coordinates>
      </Point>
    </Placemark>
    <Placemark>
      <name>Feature in other file.</name>
      <description>
        <![CDATA[ 
        Fly to a feature in another file:
        <a href="eat-at-google.kml#plymouth-rocks">Plymouth rocks</a>
        ]]>
      </description>
      <Point>
        <coordinates>-123.0821604290386,37.42041505714916</coordinates>
      </Point>
    </Placemark>
  </Document>
</kml>
