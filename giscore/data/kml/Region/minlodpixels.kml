<?xml version="1.0" encoding="utf-8"?>
<!--

This file illustrates some interesting properties of minLodPixels. Load it via
a NetworkLink and note how the overlays are triggered at the _projected_
screen sizes of regions which are much larger than the size of your physical
screen (you don't have an 8192 px monitor, do you?).

-->
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>minLodPixels</name>
    <open>1</open>
    <Folder>
      <name>minLodPixels</name>
      <open>1</open>
      <ScreenOverlay>
        <name>32</name>
        <Region>
          <LatLonAltBox>
            <north>10.0</north>
            <south>-10.0</south>
            <east>10.0</east>
            <west>-10.0</west>
          </LatLonAltBox>
          <Lod>
            <minLodPixels>32</minLodPixels>
            <maxLodPixels>64</maxLodPixels>
          </Lod>
        </Region>
        <Icon>
          <href>minlodpixels-img/32.png</href>
        </Icon>
        <overlayXY x="0" y="0" xunits="fraction" yunits="fraction" />
        <screenXY x="0" y="0" xunits="fraction" yunits="fraction" />
        <rotationXY x="0" y="0" xunits="fraction" yunits="fraction" />
        <size x="0" y="1" xunits="fraction" yunits="fraction" />
      </ScreenOverlay>
      <ScreenOverlay>
        <name>64</name>
        <Region>
          <LatLonAltBox>
            <north>10.0</north>
            <south>-10.0</south>
            <east>10.0</east>
            <west>-10.0</west>
          </LatLonAltBox>
          <Lod>
            <minLodPixels>64</minLodPixels>
            <maxLodPixels>128</maxLodPixels>
          </Lod>
        </Region>
        <Icon>
          <href>minlodpixels-img/64.png</href>
        </Icon>
        <overlayXY x="0" y="0" xunits="fraction" yunits="fraction" />
        <screenXY x="0" y="0" xunits="fraction" yunits="fraction" />
        <rotationXY x="0" y="0" xunits="fraction" yunits="fraction" />
        <size x="0" y="1" xunits="fraction" yunits="fraction" />
      </ScreenOverlay>
      <ScreenOverlay>
        <name>128</name>
        <Region>
          <LatLonAltBox>
            <north>10.0</north>
            <south>-10.0</south>
            <east>10.0</east>
            <west>-10.0</west>
          </LatLonAltBox>
          <Lod>
            <minLodPixels>128</minLodPixels>
            <maxLodPixels>512</maxLodPixels>
          </Lod>
        </Region>
        <Icon>
          <href>minlodpixels-img/128.png</href>
