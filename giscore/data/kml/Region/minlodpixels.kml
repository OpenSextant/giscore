      </ScreenOverlay>
    </Folder>
    <Placemark>
      <name>BBOX</name>
      <LineString>
        <tessellate>1</tessellate>
        <coordinates>
          -10.0,-10.0
          10.0,-10.0
          10.0,10.0
          -10.0,10.0
          -10.0,-10.0
        </coordinates>
      </LineString>
    </Placemark>
  </Document>
</kml>
