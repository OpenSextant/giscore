<?xml version="1.0" encoding="UTF-8"?>
<!-- 

Timestamps on Folders with nested Folders.
Child placemarks in containers inherit the
time interval of its parent containers.

Example shows how temporal visibility is inherited.

 Folder1 [T00:00 - 02:00]
   - placemark1 *[inherits time from folder1]
   - Folder2
       - placemark2 *[inherits time from folder1]
   - end folder2
   - placemark3 *[inherits time from folder1]
   - placemark4  [T01:00 - 03:00] [inherits end time from folder1 visible @ T01:00 - 02:00]
 end folder1
 placemark5 [T02:00 - 03:00]

-->
<kml xmlns="http://www.opengis.net/kml/2.2">
    <Document>
        <name>Inherited time</name>
        <Style id="paddle-a">
            <IconStyle>
                <Icon>
                    <href>http://maps.google.com/mapfiles/kml/paddle/A.png</href>
                </Icon>
                <hotSpot x="32" xunits="pixels" y="1" yunits="pixels"/>
            </IconStyle>
        </Style>
        <Style id="paddle-b">
            <IconStyle>
                <Icon>
                    <href>http://maps.google.com/mapfiles/kml/paddle/B.png</href>
                </Icon>
                <hotSpot x="32" xunits="pixels" y="1" yunits="pixels"/>
            </IconStyle>
        </Style>
        <Style id="hiker-icon">
            <IconStyle>
                <Icon>
                    <href>http://maps.google.com/mapfiles/ms/icons/hiker.png</href>
                </Icon>
                <hotSpot x="0" xunits="fraction" y="0.5" yunits="fraction"/>
            </IconStyle>
        </Style>
        <Folder>
            <name>Group1</name>
            <TimeSpan>
                <begin>2007-01-14T00:00:00Z</begin>
                <end>2007-01-14T02:00:00Z</end>
            </TimeSpan>
            <Placemark>
                <name>1</name>
                <styleUrl>#paddle-a</styleUrl>
                <Point>
                    <coordinates>-122.536226,37.86047,0</coordinates>
                </Point>
            </Placemark>
            <Folder>
                <name>nested folder</name>
                <Placemark>
                    <name>2</name>
                    <styleUrl>#hiker-icon</styleUrl>
                    <Point>
                        <coordinates>-122.537116,37.85961000000001,0</coordinates>
                    </Point>
                </Placemark>
            </Folder>
            <Placemark>
                <name>3</name>
                <styleUrl>#hiker-icon</styleUrl>
                <Point>
                    <coordinates>-122.537469,37.85907,0</coordinates>
                </Point>
            </Placemark>
            <!--
 NOTE placemark 4 extends beyond the lifetime of its parent folder
 so it is not visible after 02:00Z because its parent
 is not visible after that time even though the timeSpan of the
 placemark is until 03:00Z which is the latest time in the document.
-->
            <Placemark>
                <name>4</name>
                <TimeSpan>
                    <begin>2007-01-14T01:00:00Z</begin>
                    <end>2007-01-14T03:00:00Z</end>
                </TimeSpan>
                <styleUrl>#hiker-icon</styleUrl>
                <Point>
                    <coordinates>-122.537635,37.858822,0</coordinates>
                </Point>
            </Placemark>
        </Folder>
        <Placemark>
            <name>5</name>
            <TimeSpan>
                <begin>2007-01-14T02:00:00Z</begin>
                <end>2007-01-14T03:00:00Z</end>
            </TimeSpan>
            <styleUrl>#paddle-b</styleUrl>
            <Point>
                <coordinates>-122.53887,37.857604,0</coordinates>
            </Point>
        </Placemark>
    </Document>
</kml>
