<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.2">
<Document>
	<name><![CDATA[Example KML File]]></name>
	<description><![CDATA[]]></description>

	<Schema name="TrailHeadType" id="TrailHeadTypeId">          
    <SimpleField type="string" name="TrailHeadName">       
      <displayName><![CDATA[<b>Trail Head Name</b>]]></displayName>     
    </SimpleField>     
    <SimpleField type="double" name="TrailLength">       
      <displayName><![CDATA[<i>The length in miles</i>]]></displayName>     
    </SimpleField>
    <SimpleField type="int" name="ElevationGain">       
      <displayName><![CDATA[<i>change in altitude</i>]]></displayName>     
    </SimpleField>   
  </Schema> 
  <Placemark>
        <name>Simple placemark</name>
        <description>Attached to the ground. Intelligently places itself at the
          height of the underlying terrain.</description>
		<ExtendedData>                       
		  <SchemaData schemaUrl="#TrailHeadTypeId">
		    <SimpleData name="TrailHeadName">Hilton Head</SimpleData>
		    <SimpleData name="TrailLength">5.665</SimpleData>
		    <SimpleData name="ElevationGain">1503</SimpleData>
		  </SchemaData>
		</ExtendedData>
        <Point>
          <coordinates>-122.0822035425683,37.42228990140251,0</coordinates>
        </Point>
  </Placemark>
</Document>
</kml>