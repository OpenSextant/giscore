<?xml version="1.0" encoding="UTF-8"?>
<!--
 http://code.google.com/apis/kml/documentation/kml_tut.html

 take Pentagon polygon example and mix LinearRing and Polygon geometries
 to have outer ring as a LinearRing and inner ring as polygon.
 This is a contrived example.
-->
<kml xmlns="http://earth.google.com/kml/2.1">
    <Placemark>
        <name>The Pentagon - Mixed</name>
        <MultiGeometry>
            <LinearRing>
                <coordinates>
		-77.0578845766,38.8725325989,100
		-77.0546597376,38.8729101628,100
		-77.0531553685,38.8705326779,100
		-77.0555262249,38.8687578013,100
		-77.0584405629,38.8699620651,100
		-77.0578845766,38.8725325989,100
                </coordinates>
            </LinearRing>
            <Polygon>
                <extrude>1</extrude>
                <altitudeMode>relativeToGround</altitudeMode>
                <outerBoundaryIs>
                    <LinearRing>
			<coordinates>
		-77.0566805502,38.871542398,100
		-77.0554262596,38.8716789034,100
		-77.054851259,38.870765354,100
		-77.0557767743,38.8700868658,100
		-77.0569116202,38.8705444696,100
		-77.0566805502,38.871542398,100
			</coordinates>
                    </LinearRing>
                </outerBoundaryIs>
            </Polygon>
        </MultiGeometry>
    </Placemark>
</kml>
