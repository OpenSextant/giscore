  <Placemark>
    <name>My office</name>
    <description>This is the location of my office.</description>
    <Style>
      <BalloonStyle>
        <bgColor>ff7f1020</bgColor>
      </BalloonStyle>
    </Style>
    <Point>
      <coordinates>-122.0821604290386,37.42041505714916</coordinates>
    </Point>
  </Placemark>
</kml>
