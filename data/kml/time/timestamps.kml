<?xml version="1.0" encoding="UTF-8"?>
<!--
 All variations of timestamps available for kml:dateTimeType fields
 used in TimeStamp and TimeSpan elements.

	"2009", 			// gYear (YYYY)
	"2009-07", 			// gYearMonth (YYYY-MM)
	"2009-07-16",			// date (YYYY-MM-DD)
	"2009-07-16T07:30:15Z",		// dateTime (YYYY-MM-DDThh:mm:ssZ)
	"2009-07-16T10:30:15+03:00"	// dateTime (YYYY-MM-DDThh:mm:sszzzzzz)
-->
<kml xmlns="http://www.opengis.net/kml/2.2">
    <Document>
        <name>TimeStamp'ed Points</name>
        <open>1</open>
        <Placemark>
            <name>when 2009</name>
            <description>point 1</description>
            <TimeStamp>
                <when>2009</when>
            </TimeStamp>
            <Point>
                <coordinates>-122.536226,37.86047</coordinates>
            </Point>
        </Placemark>
        <Placemark>
            <name>span 2009</name>
            <description>point 2</description>
            <TimeSpan>
                <begin>2009</begin>
                <end>2009</end>
            </TimeSpan>
            <Point>
                <coordinates>-122.536422,37.860303</coordinates>
            </Point>
        </Placemark>
